<ui:ContentDialog x:Class="Fraxiinus.ReplayBook.UI.Main.Views.StaticDataDownloadDialog"
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                  xmlns:controls="clr-namespace:Fraxiinus.ReplayBook.UI.Main.Controls"
                  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                  xmlns:ui="http://schemas.modernwpf.com/2019"
                  Title="Download Static Data"
                  d:DesignHeight="450"
                  d:DesignWidth="450"
                  DefaultButton="Primary"
                  IsPrimaryButtonEnabled="True"
                  IsSecondaryButtonEnabled="True"
                  PrimaryButtonText="{DynamicResource General__DownloadButton}"
                  SecondaryButtonText="{DynamicResource CancelButtonText}"
                  mc:Ignorable="d">
    <Grid>
        <ScrollViewer>
            <ui:SimpleStackPanel Orientation="Horizontal">
                <Grid>
                    <Grid.ColumnDefinitions />
                </Grid>
                <TextBlock FontSize="16" Text="{DynamicResource ErdPresetName}" />
            </ui:SimpleStackPanel>
        </ScrollViewer>
    </Grid>
</ui:ContentDialog>
